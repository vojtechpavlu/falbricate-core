{
  "compilerOptions": {
    "target": "ES6",
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "noEmit": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "isolatedModules": true,
    "outDir": "dist",
    "moduleResolution": "node"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
